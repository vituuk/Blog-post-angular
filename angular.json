{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "ltng-news-landing-website": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss",
                    "skipTests": true
                },
                "@schematics/angular:service": {
                    "skipTests": true
                },
                "@schematics/angular:directive": {
                    "skipTests": true
                },
                "@schematics/angular:pipe": {
                    "skipTests": true
                },
                "@schematics/angular:class": {
                    "skipTests": true
                },
                "@schematics/angular:guard": {
                    "skipTests": true
                },
                "@schematics/angular:interceptor": {
                    "skipTests": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "prerender": {
                    "builder": "@@angular-devkit/build-angular:prerender",
                    "options": {
                        "browserTarget": "ltng-news-landing-website:build:production",
                        "routes": [],
                        "guessRoutes": false
                    }
                },
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "options": {
                        "outputPath": "dist/ltng-news-landing-website",
                        "index": "src/index.html",
                        "browser": "src/main.ts",
                        "polyfills": [
                          "zone.js"
                        ],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                              "glob": "**/*",
                              "input": "public"
                            }
                        ],
                        "styles": [
                          "src/styles/styles.scss"
                        ],
                        "scripts": [],
                        "stylePreprocessorOptions": {
                            "sass": {
                                "silenceDeprecations": [
                                    "import"
                                ]
                            }
                        },
                        "server": "src/main.server.ts",
                        "outputMode": "server",
                        "ssr": {
                            "entry": "src/server.ts"
                        }
                    },
                    "configurations": {
                        "production": {
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "1MB",
                                    "maximumError": "2MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kB",
                                    "maximumError": "8kB"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.prod.ts"
                            }]
                        },
                        "main": {
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "1MB",
                                    "maximumError": "2MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kB",
                                    "maximumError": "8kB"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.main.ts"
                            }]
                        },
                        "dev": {
                            "budgets": [{
                                    "type": "initial",
                                    "maximumWarning": "1MB",
                                    "maximumError": "2MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kB",
                                    "maximumError": "8kB"
                                }
                            ],
                            "outputHashing": "all",
                            "fileReplacements": [{
                                "replace": "src/environments/environment.ts",
                                "with": "src/environments/environment.dev.ts"
                            }]
                        },
                        "serve": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "development": { "buildTarget": "ltng-news-landing-website:build:dev" },
                        "production": {
                            "buildTarget": "ltng-news-landing-website:build:production"
                        },
                        "serve": {
                            "buildTarget": "ltng-news-landing-website:build:serve"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "server": {
                    "builder": "@angular-devkit/build-angular:server",
                    "options": {
                    "outputPath": "dist/ltng-news-landing-website/server",
                    "main": "server.ts"
                },
                    "configurations": {
                        "production": {
                            "outputHashing": "media"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve-ssr": {
                    "builder": "@angular-devkit/build-angular:ssr-dev-server",
                    "configurations": {
                        "development": {
                            "browserTarget": "ltng-news-landing-website:build:development",
                            "serverTarget": "ltng-news-landing-website:server:development"
                        },
                        "production": {
                            "browserTarget": "ltng-news-landing-website:build:production",
                            "serverTarget": "ltng-news-landing-website:server:production"
                        }
                    },
                    "defaultConfiguration": "development"
                },

                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": ["zone.js", "zone.js/testing"],
                        "tsConfig": "tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [{
                            "glob": "**/*",
                            "input": "public"
                        }],
                        "styles": ["src/styles/styles.scss"],
                        "scripts": []
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    }
}
